{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/koine-greek-flashcards/node_modules/.bin/next",
      "cwd": "${workspaceFolder}/koine-greek-flashcards",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/koine-greek-flashcards/.next/**/*.js"]
    }
  ]
}
